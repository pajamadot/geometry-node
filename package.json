{
  "name": "geometry-script",
  "version": "1.0.0",
  "private": true,
  "description": "A monorepo for geometry scripting with AI-powered node-based editor",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "cd apps/web && npm run dev",
    "build": "cd apps/web && npm run build",
    "start": "cd apps/web && npm run start",
    "lint": "cd apps/web && npm run lint",
    "install:web": "cd apps/web && npm install"
  },
  "keywords": [
    "geometry",
    "3d",
    "three.js",
    "react",
    "nextjs",
    "ai",
    "monorepo"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}